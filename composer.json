{
    "name": "jin2chen/form-model",
    "type": "library",
    "description": "The package helps with implementing data entry forms.",
    "keywords": [
        "validate",
        "model"
    ],
    "homepage": "https://github.com/jin2chen/form-model",
    "license": "MIT",
    "authors": [
        {
            "name": "Jin Chen",
            "email": "jin.chen@jinchen.me"
        }
    ],
    "support": {
        "issues": "https://github.com/jin2chen/form-model?state=open",
        "source": "https://github.com/jin2chen/form-model"
    },
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/validator": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "jin2chen\\FormModel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jin2chen\\FormModel\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
